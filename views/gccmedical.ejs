<!doctype html>
<html class="no-js" lang="en">

<%-include('./partials/header.ejs')%>

<body>
   <%-include('./partials/leftsidebar.ejs')%>

   <%- include('./partials/topnav.ejs')%>

  
    <div class="all-content-wrapper">
        <div class="container-fluid">
            <div class="product-payment-inner-st">
                <h2 class="form-title">Send candidates for GCC</h2>
                <div class="chosen-select-single mg-b-20">
                    <select data-placeholder="Select Candidates" class="chosen-select" multiple="" tabindex="-1">
                        <option value="">Select</option>
                            <% candidate.forEach(cand=>{ %>
                                <option value="<%=cand.name%>"><%= cand.name +" "+ cand.passportnumber %></option>
                            <%})%>
                        </select>
                </div>
                <div class="form-group">
                    <input name="passportnumber" type="text" maxlength="8" minlength="8" required class="form-control" placeholder="Passport number">
                </div>
               
            </div>
        </div>
    </div>

  <%-include('./partials/footer.ejs')%>
</body>

</html>